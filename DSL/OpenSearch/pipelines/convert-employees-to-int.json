{
  "description": "Pipeline that converts workers from string to integer",
  "processors": [
    {
      "convert": {
        "field": "workers",
        "type": "integer",
        "target_field": "workers"
      }
    }
  ],
  "on_failure": [
    {
      "set": {
        "field": "workers",
        "value": 0
      }
    }
  ]
}
